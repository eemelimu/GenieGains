{
  "expo": {
    "name": "GymJunkie",
    "slug": "GymJunkie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/GymJunkieLogo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/GymJunkieLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/GymJunkieLogo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.gymjunkie.GymJunkie",
      "permissions": ["android.permission.CAMERA"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["react-native-vision-camera"],
  }
}
